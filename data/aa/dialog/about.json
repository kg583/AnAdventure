{
    "type": "minecraft:multi_action",
    "title": "AnAdventure",
    "body": [
        {
            "type": "minecraft:plain_message",
            "contents": {
                "translate": "text.aa.description",
                "fallback": "Minecraft has many stories, but this one is ours."
            },
            "width": 300
        },
        {
            "type": "minecraft:plain_message",
            "contents": {
                "translate": "version.aa.number",
                "fallback": "-- Error --",
                "extra": [
                    {
                        "text": "\n"
                    },
                    {
                        "translate": "version.aa.authors",
                        "fallback": "Missing resource pack"
                    }
                ],
                "color": "gray"
            },
            "width": 300
        }
    ],
    "actions": [
        {
            "label": {
                "translate": "text.aa.website",
                "fallback": "Website"
            },
            "action": {
                "type": "minecraft:open_url",
                "url": "https://github.com/kg583/AnAdventure"
            }
        },
        {
            "label": {
                "translate": "text.aa.issues",
                "fallback": "Issues"
            },
            "action": {
                "type": "minecraft:open_url",
                "url": "https://github.com/kg583/AnAdventure/issues"
            }
        }
    ],
    "exit_action": {
        "action": {
            "type": "minecraft:show_dialog",
            "dialog": "smithed:data_packs"
        },
        "label": {
            "translate": "gui.back"
        },
        "width": 200
    }
}